	<RULE NAME="comercio aceptar" TOPLEVEL="INACTIVE">
		<RULEREF NAME="aceptar o cancelar"/>
	</RULE>
	<RULE NAME="si o no">
		<L PROPNAME="si o no">
			<P VALSTR="si">si</P>
			<P VALSTR="no">no</P>
		</L>
	</RULE>
	<RULE NAME="aceptar o cancelar">
		<L PROPNAME="aceptar o cancelar">
			<P VALSTR="aceptar" ><P>este</P><P>si</P></P>
			<P VALSTR="cancelar"><P>este</P><P>no</P></P>
		</L>	
	</RULE>
	<RULE NAME="navega cliente"  TOPLEVEL="INACTIVE">
		<L PROPNAME="navega cliente" >
			<P VALSTR="Anterior" >cliente anterior</P>
			<P VALSTR="Siguiente">cliente siguiente</P>
			<P VALSTR="Aceptar"  >cliente aceptar</P>
		</L>	
	</RULE>
	<RULE NAME="lote numeric" TOPLEVEL="INACTIVE">
		<P>lote</P>
		<RULEREF NAME="numero 0-999999"/>
	</RULE>
	<RULE NAME="lote"  TOPLEVEL="INACTIVE">
		<P>lote</P>
		<RULEREF PROPNAME="LotePart1"  NAME="diez_diezinueve"/>
		<P>cero</P>
		<RULEREF PROPNAME="LotePart2"  NAME="diez_diezinueve"/>
	</RULE>
