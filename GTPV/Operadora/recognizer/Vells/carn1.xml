<GRAMMAR>
	<RULE NAME="hola" TOPLEVEL="INACTIVE">
		<P>hola</P>
	</RULE>
	<RULE NAME="pedir nombres" TOPLEVEL="INACTIVE">
		<L PROPNAME="nombre">
		</L>
	</RULE>
	<RULE NAME="reparto" TOPLEVEL="INACTIVE">
		<L PROPNAME="reparto no">
			<P VALSTR="reparto">reparto</P>
			<P VALSTR="no">no</P>
		</L>
	</RULE>
	<RULE NAME="si no" TOPLEVEL="INACTIVE">
		<L PROPNAME="si no">
			<P VALSTR="si">si</P>
			<P VALSTR="no">no</P>
		</L>
	</RULE>
	<RULE NAME="lote" TOPLEVEL="INACTIVE">
		<RULEREF NAME="numero 0-999999"/>
	</RULE>
	<RULE NAME="peso" TOPLEVEL="INACTIVE">
		<RULEREF NAME="numero 0-999999"/>
	</RULE>
	<RULE NAME="numero 0-999999">
		<L>
			<RULEREF NAME="millon"/>
			<P PROPNAME="cero">cero</P>
		</L>
	</RULE>
	<RULE NAME="millon">
		<L>
			<P>
				<P PROPNAME="M">
					<L>
						<RULEREF NAME="millar"/>
						<P PROPNAME="U" VAL="1"/>
					</L>	
				</P>				
				<P>mil</P>
				<O><RULEREF NAME="millar"/></O>
			</P>
			<RULEREF NAME="millar"/>
		</L>
	</RULE>
	<RULE NAME="millar">
		<L>
			<P>
				<P PROPNAME="C">
					<L PROPNAME="U">
						<P VAL="1">cien</P>
						<P VAL="1">ciento</P>
						<P VAL="2">doscientos</P>
						<P VAL="3">trescientos</P>
						<P VAL="4">cuatrocientos</P>
						<P VAL="5">quinientos</P>
						<P VAL="6">seiscientos</P>
						<P VAL="7">setecientos</P>
						<P VAL="8">ochocientos</P>
						<P VAL="9">novecientos</P>
					</L>						
				</P>
				<O><RULEREF NAME="centena"/></O>
			</P>
			<RULEREF NAME="centena"/>
		</L>		
	</RULE>			
	<RULE NAME="centena">
		<L>
			<RULEREF NAME="decena"/>
			<RULEREF NAME="diez_diezinueve"/>
			<RULEREF NAME="veinte_noventainueve"/>
		</L>
	</RULE>
	<RULE NAME="decena">
		<L PROPNAME="U">
			<P VAL="1">uno</P>
			<P VAL="1">un</P>
			<P VAL="2">dos</P>
			<P VAL="3">tres</P>
			<P VAL="4">cuatro</P>
			<P VAL="5">cinco</P>
			<P VAL="6">seis</P>
			<P VAL="7">siete</P>
			<P VAL="8">ocho</P>
			<P VAL="9">nueve</P>
		</L>
	</RULE>
	<RULE NAME="diez_diezinueve">
		<P PROPNAME="D"><P PROPNAME="U" VAL="1"/></P>
		<L PROPNAME="U">
			<P VAL="0">diez</P>
			<P VAL="1">once</P>
			<P VAL="2">doce</P>
			<P VAL="3">trece</P>
			<P VAL="4">catorze</P>
			<P VAL="5">quince</P>
			<P VAL="6">dieziseis</P>
			<P VAL="7">diezisiete</P>
			<P VAL="8">dieziocho</P>
			<P VAL="9">diezinueve</P>
		</L>
	</RULE>
	<RULE NAME="veinte_noventainueve">
		<P PROPNAME="D">
			<L PROPNAME="U">
				<P VAL="2">veinte</P>
				<P VAL="2">veinti</P>
				<P VAL="3">treinta</P>
				<P VAL="4">cuarenta</P>
				<P VAL="5">cincuenta</P>
				<P VAL="6">sesenta</P>
				<P VAL="7">setenta</P>
				<P VAL="8">ochenta</P>
				<P VAL="9">noventa</P>
			</L>
		</P>
		<O>i</O>
		<O><RULEREF NAME="decena"/></O>
	</RULE>
</GRAMMAR>

